{
  "name": "ui5-cap-event-app-server",
  "version": "0.0.0",
  "description": "A simple CAP-based UI5 Event Application Server.",
  "private": true,
  "dependencies": {
    "@sap-ux/ui5-proxy-middleware": "^1.0.1",
    "@sap/cds": "^4.6.6",
    "express": "*",
    "passport": "^0.4.1",
    "sqlite3": "^5.0.0"
  },
  "scripts": {
    "cds:build": "cds build",
    "cds:start": "cds run",
    "cds:watch": "cds watch",
    "cds:debug": "node --inspect bin/cds run",
    "cds:debug-brk": "node --inspect-brk bin/cds run",
    "start": "npm run cds:start"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sql",
        "model": [
          "db",
          "srv"
        ]
      }
    }
  }
}
